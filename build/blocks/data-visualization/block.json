{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "agb/data-visualization",
  "title": "Data Visualization",
  "category": "advanced-gutenberg-blocks",
  "icon": "chart-bar",
  "description": "Create interactive charts and data visualizations",
  "keywords": [
    "chart",
    "graph",
    "data",
    "visualization"
  ],
  "textdomain": "advanced-gutenberg-blocks",
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "attributes": {
    "chartType": {
      "type": "string",
      "default": "bar"
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "chartData": {
      "type": "array",
      "default": []
    },
    "height": {
      "type": "number",
      "default": 400
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}